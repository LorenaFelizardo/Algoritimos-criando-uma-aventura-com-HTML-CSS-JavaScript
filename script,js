constante avanca = documento.consultaSeletorTodos('.btn-proximo');

avanca.para cada(botão => {
    botão.adicionarEventListener('clique', função(){
        constantte atual = document.querySelector('.ativo');
        constante próximoPasso = 'passo-' + esse.obterAtributo('dados-próximos');

        atual.lista de classe.remover('ativo');
        documento.obterElementoPorId(proximoPasso).lista de classe.adicionar('ativo');
    })
})
